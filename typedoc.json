{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "typedoc/",
  "plugin": [
    "typedoc-plugin-extras",
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-mdn-links"
  ],

  //   typedoc-plugin-extras
  "footerTypedocVersion": true,
  "footerDate": true,
  "footerTime": true
}
