    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "npmignore": "^0.3.0",
    "prettier": "^2.7.1",
    "typedoc": "^0.23.10",
    "typedoc-plugin-extras": "^2.3.0",
    "typedoc-plugin-mdn-links": "^2.0.0",
