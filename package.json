{
  "name": "better-trakt",
  "version": "0.3.1",
  "description": "A Trakt.tv client with native Typescript support and quality of life features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/getaugur/better-trakt.git",
